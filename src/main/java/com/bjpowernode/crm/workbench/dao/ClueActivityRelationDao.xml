<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bjpowernode.crm.workbench.dao.ClueActivityRelationDao">
	

    <delete id="unBund">
        delete from tbl_clue_activity_relation where id =#{id}
    </delete>

    <insert id="doBund" >
        insert into tbl_clue_activity_relation (id, clueId, activityId)
        values (#{id}, #{clueId}, #{activityId})
    </insert>

    <select id="selectActivityList" resultType="com.bjpowernode.crm.workbench.domain.ClueActivityRelation">
        select * from tbl_clue_activity_relation where clueId=#{clueId}
    </select>
    <delete id="delete">
        delete from tbl_clue_activity_relation where id=#{id}
    </delete>

</mapper>

































