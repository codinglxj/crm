<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjpowernode.crm.settings.dao.DicValueDao">
    <select id="getAllValue" resultType="com.bjpowernode.crm.settings.domain.DicValue">
        select
        id,
        value,
        text,
        orderNo,
        typeCode from tbl_dic_value where typecode=#{type} order by orderNo

    </select>
</mapper>